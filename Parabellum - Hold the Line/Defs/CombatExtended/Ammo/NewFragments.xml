<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseFragment">
		<defName>Fragment_Cluster</defName>
		<label>Cluster fragments</label>
		<graphicData>
			<texPath>Things/Projectile/CGR/Dart</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<speed>24</speed>
			<armorPenetrationSharp>4</armorPenetrationSharp>
			<armorPenetrationBlunt>12</armorPenetrationBlunt>
			<gravityFactor>12</gravityFactor>
		</projectile>
		<comps>
			<li Class="CombatExtended.CompProperties_ExplosiveCE">
				<damageAmountBase>120</damageAmountBase>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explosiveRadius>2.1</explosiveRadius>
			</li>
			<li Class="CombatExtended.CompProperties_Fragments">
				<fragments>
					<Fragment_Large>10</Fragment_Large>
				</fragments>
			</li>
		</comps>
	</ThingDef>
	
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseFragment">
		<defName>Fragment_Plastic</defName>
		<label>Plastic Flakes</label>
		<graphicData>
			<texPath>Things/Projectile/Bullet_big</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Beanbag</damageDef>
			<damageAmountBase>32</damageAmountBase>
			<speed>192</speed>
			<armorPenetrationSharp>4</armorPenetrationSharp>
			<armorPenetrationBlunt>60</armorPenetrationBlunt>
			<gravityFactor>15</gravityFactor>
		</projectile>
	</ThingDef>
	
</Defs>